        <form name="bodyFORM" method="post" onsubmit="return false;">
            <!-- contents_body -->
            































<script>
function funcLinkMenuId(home_gb, func, gaebalwon, gwajeong, searchValue){
    var formURL = "/main/getLinkMenuIdList.do";
    $.ajax({
        url : formURL,
        type: "POST",
        data: ({"home_gb" : home_gb}),
        success:function(responseText, textStatus, jqXHR){
            var responseResult = eval("(" + responseText + ")");
            var totalCount = responseResult.TOTAL_COUNT;

            if(totalCount > 0){
                var v_rootMenuId = "";
                var v_menuId = "";
                var v_menuTypeValue = "";
                var chk_index = true;
                for(i=0; i<totalCount; i++){
                    //home_gb 부가기능(1:메뉴명 노출 안함, 2:양성과정 URL 활용, 3:향상과정 URL 활용)';
                    var MENU_TYPE_VALUE = responseResult.DATA_LIST[i].MENU_TYPE_VALUE;
                    //alert("MENU_TYPE_VALUE="+MENU_TYPE_VALUE);
                    //alert("MENU_TYPE_VALUE.indexOf(searchValue)="+MENU_TYPE_VALUE.indexOf(searchValue));
                    if(home_gb == '2'){
                        if(chk_index && MENU_TYPE_VALUE.indexOf(searchValue) >= 0){
                            v_rootMenuId = responseResult.DATA_LIST[i].CATEGORY_MENU_NO;
                            v_menuId = responseResult.DATA_LIST[i].MENU_NO;
                            v_menuTypeValue = responseResult.DATA_LIST[i].MENU_TYPE_VALUE;
                            chk_index = false;
                        }
                    }

                    if(home_gb == '3'){
                        if(chk_index && MENU_TYPE_VALUE.indexOf(searchValue) >= 0){
                            v_rootMenuId = responseResult.DATA_LIST[i].CATEGORY_MENU_NO;
                            v_menuId = responseResult.DATA_LIST[i].MENU_NO;
                            v_menuTypeValue = responseResult.DATA_LIST[i].MENU_TYPE_VALUE;
                            chk_index = false;
                        }
                    }
                }

                if(func == 'funcDetail'){ //양성 교육과정 상세
                    funcDetail(v_rootMenuId, v_menuId, v_menuTypeValue, gaebalwon, gwajeong);
                } else if(func == 'funcDetailCourse'){ //향상 교육과정 상세
                    funcDetailCourse(v_rootMenuId, v_menuId, v_menuTypeValue, gaebalwon, gwajeong);
                }
            }
        },
        error: function(jqXHR, textStatus, errorThrown){
            popOpen('.popBase','확인',"에러가 발생하였습니다....\n"+textStatus);
        }
    });
}

//양성 교육과정 상세
function funcDetail(rootMenuId, menuId, menuTypeValue, gaebalwon_cd, gwajeong_no){
    var form = document.bodyFORM;

    var params  = "";
        params += "?rootMenuId="+rootMenuId;
        params += "&menuId="+menuId;
        params += "&gaebalwon_cd="+gaebalwon_cd;
        params += "&gwajeong_no="+gwajeong_no;

    form.method = 'post';
    form.target = "_self";
    form.action ='/education/professionalSkillEduDetail.do'+params;
    form.submit();
}

//향상 교육과정 상세
function funcDetailCourse(rootMenuId, menuId, menuTypeValue, gaebalwon_cd, mcourse_no){
    var form = document.bodyFORM ;

    var params  = "";
        params += "?rootMenuId="+rootMenuId;
        params += "&menuId="+menuId;
        params += "&gaebalwon_cd="+gaebalwon_cd;
        params += "&mcourse_no="+mcourse_no;
        params += "&callFlag=YEAR";
        params += "&"+menuTypeValue;

    form.method = "post";
    form.target = "_self";
    form.action = "/education/improvementEduDetail.do"+params;
    form.submit();
}

//양성 교육과정 목록
function funcSearch(keyword){
    var form = document.bodyFORM ;

    var chk = true;
    if(keyword == 'SEARCH'){
        if(form.mgwajeong_nm.value == ''){
            chk = false;
            alert('찾고 싶은 키워드를 입력하세요.');
        }
    } else {
        form.mgwajeong_nm.value = keyword;
    }

    if(chk){
        form.method = "post";
        form.target = "_self";
        form.action = "/education/professionalSkillEduList.do?rootMenuId=3002&menuId=3008&mojip_cd=111001";
        form.submit();
    }
}


//video
function funcVideo(iframe_no){
    var video_url = "";
    if(iframe_no == 1) video_url = "https://www.youtube.com/embed/1Qql7gCxSsM";
    if(iframe_no == 2) video_url = "https://www.youtube.com/embed/0d_PfsrCCas";
    if(iframe_no == 3) video_url = "https://www.youtube.com/embed/GVR4Imz-Aak";

    for(var i=1; i<4; i++){
        if(i == iframe_no){
            $('#video'+i).attr("src", video_url + "?autoplay=1&mute=1");
        } else {
            $('#video'+i).attr("src", "");
        }
    }
}

//공지사항
function  funcGoBbsDetail(bbs_id, bbs_sno, group_bbs_id,link_menu){
    var form = document.bodyFORM ;

    var params  = "";
        params += "?"+link_menu;
        params += "&bbs_id="+bbs_id;
        params += "&currentPage=1";
        params += "&customer_page_list=";
        params += "&bbs_sno="+bbs_sno;

    form.method = "post";
    form.target = "_self";
    form.action = "/bbs/bbsDetail.do"+params;
    form.submit();
}
</script>


