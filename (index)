<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"/>
<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
<script>
    $(document).ready(function(){
        AOS.init({
            offset: 200,
            duration: 1000,
            easing: 'ease-in-out',
            anchorPlacement: 'top-center'
        });
        const swiperMain = new Swiper('.swiper-main', {
            navigation: {
                nextEl: ".swiper-button-next-main",
                prevEl: ".swiper-button-prev-main",
            },
            pagination: {
                type: "fraction",
                el: ".swiper-pagination",
                clickable: true,
                bulletActiveClass: "swiper-pagination-bullet-active",
                progressbarFillClass: "swiper-pagination-progressbar-fill",
                formatFractionCurrent: function (number) {
                    return '0' + number;
                },
                formatFractionTotal: function (number) {
                    return '0' + number;
                },
                renderFraction: function (currentClass, totalClass) {
                    return '<span class="' + currentClass + '"></span>' + "<span class='bar'> / </span>" +'<span class="' + totalClass + '"></span>';
                }
            },
            autoplay: {
                delay: 5000,
            },
            speed: 500,
            loop: true,
            spaceBetween: 0,
        });
        const swiper_content = new Swiper('.slide_wrap', {
            slidesPerView: 4,
            navigation: {
                nextEl: ".swiper-button-next-content",
                prevEl: ".swiper-button-prev-content",
            },
            loop:true,
            autoplay: {
                delay: 2000,
                disableOnInteraction: false,
            },
            spaceBetween: 30,
            breakpoints: {
                0: {
                    slidesPerView: 2,
                },
                992: {
                    slidesPerView: 4,
                },
            },
        });
        $('.slide_wrap').hover(function() {
            swiper_content.autoplay.stop();
        }, function(){
            swiper_content.autoplay.start();
        });
        const swiper_rolling = new Swiper('.rolling_slide', {
            direction: "vertical",
            slidesPerView: 1,
            loop:true,
            spaceBetween: 30,
            autoplay: {
                delay: 3000,
            },
            speed: 500,
        });
        const swiper_promotion = new Swiper('.in_promotion', {
            slidesPerView: 2,
            navigation: {
                nextEl: ".swiper-button-next-promotion",
                prevEl: ".swiper-button-prev-promotion",
            },
            loop:true,
            spaceBetween: 30,
            autoplay: {
                delay: 4000,
            },
            speed: 500,
            breakpoints: {
                0: {
                    slidesPerView: 1,
                },
                992: {
                    slidesPerView: 2,
                },
            },
        });
        
        var Stops = document.querySelector('.stopbtn');
        var Plays = document.querySelector('.playbtn');

        const swiper_advertise = new Swiper('.advertise_slide_area', {
            slidesPerView: 1,
            navigation: {
                nextEl: ".swiper-button-next-advertise",
                prevEl: ".swiper-button-prev-advertise",
            },
            pagination: {
                type: "fraction",
                el: ".swiper-pagination-advertise",
                clickable: true,
                bulletActiveClass: "swiper-pagination-bullet-active",
                progressbarFillClass: "swiper-pagination-progressbar-fill",
            },
            autoplay: {
                delay: 5000,
            },
            speed: 500,
            loop: true,
            spaceBetween: 30,
            on:{
                autoplayStart: (swiper) => {
                Plays.classList.remove('active');
                Stops.classList.add('active');
                },
                autoplayStop: (swiper) => {
                    Plays.classList.add('active');
                    Stops.classList.remove('active');
                },
            }
        });
        window.onload = function() {
            Stops.addEventListener('click', () => { swiper_advertise.autoplay.stop() });
            Plays.addEventListener('click', () => { swiper_advertise.autoplay.start() });
        }

        const swiper_footer = new Swiper('.f_in_slide', {
            slidesPerView: 4,
            navigation: {
                nextEl: ".swiper-button-next-footer",
                prevEl: ".swiper-button-prev-footer",
            },
            autoplay: {
                delay: 5000,
            },
            speed: 500,
            loop: true,
            spaceBetween: 30,
            breakpoints: {
                0: {
                    slidesPerView: 2,
                },
                992: {
                    slidesPerView: 4,
                },
            },
        });
    });
    $(window).scroll(function() {
        var scrolltop = $(window).scrollTop();
        var height = $('.swiper-main').height();
        if(scrolltop > height){
            $('#quick_menu').addClass('ds-b');
            $('#quick_menu').removeClass('ds-n');
        }else{
            $('#quick_menu').removeClass('ds-b');
            $('#quick_menu').addClass('ds-n');
        }
    });
</script>
<script src="/js/main24/page.js"></script>
